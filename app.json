{
  "expo": {
    "name": "Stanford Daily",
    "description": "The Stanford Daily Mobile App",
    "slug": "StanfordDaily",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "version": "1.1.3",
    "platforms": [
      "ios",
      "android"
    ],
    "githubUrl": "https://github.com/TheStanfordDaily/StanfordDaily_Mobile",
    "orientation": "portrait",
    "primaryColor": "#94171C",
    "icon": "./assets/icon.png",
    "scheme": "stanforddaily",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#94171C"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#780A11"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 30000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.Stanford.Daily.App",
      "appStoreUrl": "https://itunes.apple.com/app/stanford-daily/id1341270063",
      "supportsTablet": true
    },
    "android": {
      "package": "com.Stanford.Daily.App",
      "versionCode": 5,
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.Stanford.Daily.App",
      "permissions": [
        "ACCESS_COARSE_LOCATION"
      ]
    }
  }
}
