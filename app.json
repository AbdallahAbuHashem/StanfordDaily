{
  "expo": {
    "name": "Stanford Daily",
    "description": "The Stanford Daily Mobile App",
    "slug": "StanfordDaily",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#780A11"
    },
    "updates": {
      "fallbackToCacheTimeout": 5000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Stanford.Daily.App"
    },
    "android": {
      "package": "com.Stanford.Daily.App",
      "versionCode": 3,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
      ]
    }
  }
}
